                    1200: {
                        items: 4
                    }
                }
            });
            $('#tsp_productrelated div.products-grid').removeClass('owl_loading');
        });;
        jQuery(document).ready(function($) {;
            (function(el) {
                var $el = $(el),
                    $tab = $('.tab', $el),
                    $tab_content = $('.tab-content', $el),
                    $tab_content_inner = $('.tab-content-inner', $tab_content);
                $tab.on('click.tab_cat', function() {
                    var $this = $(this);
                    if ($this.hasClass('active')) return;
                    $tab.removeClass('active');
                    $this.addClass('active');
                    var id_tab = $this.attr('data-id'),
                        $tab_content_active = $('.tab-content-' + id_tab, $el);
                    $tab_content_inner.removeClass('active').css({
                        'overflow': 'hidden',
                        'height': '0'
                    });
                    $tab_content_active.addClass('active').css({
                        'overflow': '',
                        'height': ''
                    });
                    var $loading = $('.process-loading', $tab_content_active);
                    var loaded = $tab_content_active.hasClass('ltabs-items-loaded');
                    if (!$this.hasClass('tab-loaded') && !$this.hasClass('tab-process')) {
                        $this.addClass('tab-process');
                        $loading.show();
                        ajax_url = baseDir + 'modules/tspproducttabsslider/tspproducttabsslider-ajax.php';
                        $.ajax({
                            type: 'POST',
                            url: ajax_url,
                            data: {
                                module_name: 'tspproducttabsslider',
                                is_ajax: 1,
                                ajax_start: 0,
                                categoryid: $this.attr('data-catid'),
                                data_type: $this.attr('data-type')
                            },
                            success: function(data) {
                                if (data.productList != '') {
                                    $(data.productList).insertBefore($loading);
                                    $this.addClass('tab-loaded').removeClass('tab-process');
                                    $loading.remove();
                                } else {
                                    $('<p class="alert alert-info">There are no products matching to show.</p>').insertBefore($loading);
                                    $loading.remove();
                                }
                            },
                            dataType: 'json'
                        });
                    }
                });
            })('#tsp_producttabsslider');
        });;
        $(window).on('load resize', function() {
            var offset_l = $('#tsp_scwrapper_926801526561571').offset().left;
            var wrap_offset_l = $('#tsp_wrapper').offset().left;
            var left = offset_l - wrap_offset_l;
            $('#tsp_scwrapper_926801526561571 .tsp_scwrapper_bg').css({
                'left': -left,
                'width': $('#tsp_wrapper').outerWidth()
            });
        });
